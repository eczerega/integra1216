<p id="notice"><%= notice %></p>

<h1>Listing Sku Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Sku</th>
      <th>Stock</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sku_stocks.each do |sku_stock| %>
      <tr>
        <td><%= sku_stock.SKU %></td>
        <td><%= sku_stock.stock %></td>
        <td><%= link_to 'Show', sku_stock %></td>
        <td><%= link_to 'Edit', edit_sku_stock_path(sku_stock) %></td>
        <td><%= link_to 'Destroy', sku_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sku stock', new_sku_stock_path %>
