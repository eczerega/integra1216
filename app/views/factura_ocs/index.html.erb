
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Dashboard <small>Ordenes completadas</small>
        </h1>
        <ol class="breadcrumb">
            <li class="active">
                <i class="fa fa-dashboard"></i> /Dashboard/ordenes_completas
            </li>
        </ol>
    </div>
</div>
<!-- /.row -->
<div class="container">
<table class="table table-striped">
  <thead >
    <tr>
      <th>Factura</th>
      <th>Oc</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @factura_ocs.each do |factura_oc| %>
      <tr>
        <td><%= factura_oc.factura_id %></td>
        <td><%= factura_oc.oc_id %></td>
        <td><%= factura_oc.estado %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
